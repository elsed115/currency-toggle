{
    "name": "elsed115/currency-toggle",
    "description": "A Laravel Nova Currency Field with an activate/deactivate toggle",
    "keywords": [
        "laravel",
        "nova"
    ],
    "type": "nova-field",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Elsed\\CurrencyToggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elsed\\CurrencyToggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}


